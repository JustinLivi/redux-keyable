{
  "name": "redux-keyable",
  "version": "0.2.2",
  "description": "Small typescript redux utility to improve typesafety between action creators and reducers.",
  "main": "build/index.js",
  "types": "types",
  "typesVersions": {
    ">=3.1": {
      "*": [
        "types/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc && ts-node ./scripts/addHeader.ts",
    "test": "jest --coverage && npm run tslint && npm run dtslint",
    "watch": "jest --watch",
    "cover": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "dtslint": "npm run build && dtslint types; git checkout build && git checkout types/index.d.ts",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "preversion": "npm run build && git add -A",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustinLivi/redux-keyable.git"
  },
  "keywords": [
    "redux",
    "typescript",
    "typesafe",
    "keyable",
    "reducer",
    "action",
    "creator",
    "flux"
  ],
  "author": "Justin Livi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JustinLivi/redux-keyable/issues"
  },
  "homepage": "https://github.com/JustinLivi/redux-keyable#readme",
  "dependencies": {
    "@types/lodash": "^4.14.120",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.0",
    "coveralls": "^3.0.3",
    "dtslint": "^0.5.5",
    "fs-extra": "^7.0.1",
    "jest": "^24.1.0",
    "jest-extended": "^0.11.1",
    "np": "^4.0.2",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.4"
  }
}
